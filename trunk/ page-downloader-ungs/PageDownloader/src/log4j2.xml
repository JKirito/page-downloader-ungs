<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO">
<Appenders>
    <Console name="Console" target="SYSTEM_OUT">
        <PatternLayout pattern="%msg%n"/>
    </Console>
    <File name="myName" fileName="vghjv.log" immediateFlush="true" append="true">
        <PatternLayout pattern="%msg%n"/>
    </File>
</Appenders>
<Loggers>
    <Logger level="info">
        <AppenderRef ref="Console"/>
        <AppenderRef ref="File"/>
    </Logger>
    <Root level="trace">
        <AppenderRef ref="Console"/>
    </Root>
</Loggers>
</Configuration>